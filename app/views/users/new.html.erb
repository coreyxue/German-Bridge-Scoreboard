<h1>Sign up all player</h1>
<%=link_to "Home", '/'%><br/>
<%if @users %>
	<% for @u in @users %>
		<br/><%=@u.name%>
	<%end%>
<%end%>
<%= form_for(@user) do |f| %>
<div id="name_entry">
	<%= f.label :name %>
	<%= f.text_field :name,id:"name_box"%><br/>
	<div id="next_but">
		<%= f.submit "Next" %>
	</div>
	<%= f.submit "End" %>
</div>
<%end%>